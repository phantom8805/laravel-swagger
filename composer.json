{
  "name": "ronasit/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": ["laravel", "swagger", "testing", "auto-documentation", "rest-api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.3.0",
    "minime/annotations": "2.3.1",
    "ronasit/laravel-helpers": "1.1.*@beta",
    "zircote/swagger-php": "~2.0|3.*",
    "swagger-api/swagger-ui": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\AutoDoc\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\AutoDoc\\AutoDocServiceProvider"
      ]
    }
  }
}
